{
  "name": "sdlfirst",
  "version": "0.0.1",
  "description": "GraphQL SDL-first Tools",
  "keywords": [
    "graphql",
    "sdl",
    "typescript"
  ],
  "homepage": "https://github.com/trevorr/sdlfirst",
  "bugs": "https://github.com/trevorr/sdlfirst/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorr/sdlfirst.git"
  },
  "license": "ISC",
  "author": "Trevor Robinson",
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json",
    "sdl"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sdlfirst": "./bin/run"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project src",
    "lint": "eslint 'src/**/*.ts'",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "prettier": "prettier --write 'src/**/*.ts'",
    "start": "ts-node src/index.ts",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif-dev readme && git add README.md"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@graphql-codegen/core": "^2.6.8",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-tools/graphql-file-loader": "^7.5.14",
    "@graphql-tools/load": "^7.8.9",
    "@graphql-tools/schema": "^9.0.13",
    "@oclif/command": "^1.8.21",
    "@oclif/config": "^1.18.6",
    "@oclif/plugin-help": "^5.2.1",
    "diff": "^5.1.0",
    "graphql": "^16.6.0",
    "pascal-case": "^3.1.2",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.3",
    "snake-case": "^3.0.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "typescript-memoize": "^1.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.3.3",
    "@types/chai": "^4.3.4",
    "@types/diff": "^5.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^11.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "sdlfirst",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
